<Window x:Class="DotAgro.Dialogs.HeadquarterManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DotAgro.Dialogs"
        mc:Ignorable="d"
        Title="HeadquarterManager" Height="160" Width="500">
    <Grid>
        <StackPanel>
            <TextBlock Margin="0,20,0,20" TextAlignment="Center" Text="Siège"/>
            <DockPanel Margin="5,2,5,2">
                <Label Content="Ville : " Margin="0,0,49,0"/>
                <TextBox x:Name="cityBox" Margin="5"/>
            </DockPanel>
            <DockPanel Margin="20" HorizontalAlignment="Center">
                <Button x:Name="quitClick" Content="Quitter" Width="150" Margin="30,0,30,0"/>
                <Button x:Name="validClick" Content="Valider" Width="150" Margin="30,0,30,0"/>
            </DockPanel>
        </StackPanel>
    </Grid>
</Window>
